 function formatDateFromTimestamp(timestamp) {
  // Create a new Date object using the timestamp
  const date = new Date(timestamp);

  // Extract the year, month, and day components from the Date object
  const year = date.getFullYear();
  // JavaScript months are zero-based, so we add 1 to get the correct month
  const month = String(date.getMonth() + 1).padStart(2, '0'); // Zero-padding if needed
  const day = String(date.getDate()).padStart(2, '0'); // Zero-padding if needed

  // Concatenate the components in the desired format
  const formattedDate = `${year}-${month}-${day}`;

  return formattedDate;
}

function getDay(timestammp){
  const date = new Date(1715804139*1000);
  const day = date.getDate();
  return day;
}

 console.log(getDay(Date.now()));


