async function createDefaultNodeInRealtimeDatabase() {
  const database = admin.database();
  const motorSetting = {
      device_status:2,
      from_mobile:false,
      output_status:2,
      CF:0,
      VoltageFaultStatus:2,
      voltage:230.21,
      mobile_status:2,
      motorAmps:0.1
  }
  const motorParameter = {
      maxAmps:0,
      minAmps:0,
      timeStamp:1234
  }

  const tankSetting = {
      actual_height : 350,
      float_status: 2,
      level: 155,
      random: 222
  }


  const tankParameter = {
      actualHeight : 350,
      maxHeight: 300,
  }

  try {
    // Set the default data at the desired location in the Realtime Database


    await database.ref('site2/motor1').set(motorSetting);

    await database.ref('site2/motor1'+'_Parameter').set(motorParameter);

    await database.ref('site2/tank1').set(tankSetting);

    await database.ref('site2/tank1'+'_Parameter').set(tankParameter);

    await database.ref('site2/motor2').set(motorSetting);

    await database.ref('site2/motor2'+'_Parameter').set(motorParameter);

    await database.ref('site2/motor3').set(motorSetting);

    await database.ref('site2/motor3'+'_Parameter').set(motorParameter);

    await database.ref('site2/motor4').set(motorSetting);

    await database.ref('site2/motor4'+'_Parameter').set(motorParameter);

    await database.ref('site2/motor5').set(motorSetting);

    await database.ref('site2/motor5'+'_Parameter').set(motorParameter);

    await database.ref('site2/filter1').set(motorSetting);

    await database.ref('site2/filter1'+'_Parameter').set(motorParameter);

    await database.ref('site2/filter2').set(motorSetting);

    await database.ref('site2/filter2'+'_Parameter').set(motorParameter);

    await database.ref('site2/filter3').set(motorSetting);

    await database.ref('site2/filter3'+'_Parameter').set(motorParameter);

    await database.ref('site2/tank2').set(tankSetting);

    await database.ref('site2/tank2'+'_Parameter').set(tankParameter);

    await database.ref('site2/tank3').set(tankSetting);

    await database.ref('site2/tank3'+'_Parameter').set(tankParameter);

    await database.ref('site2/tank5').set(tankSetting);

    await database.ref('site2/tank5'+'_Parameter').set(tankParameter);

    await database.ref('site2/tank4').set(tankSetting);

    await database.ref('site2/tank4'+'_Parameter').set(tankParameter);

    console.log("Everything setup");

  } catch (error) {
    console.error('Error creating default node in Realtime Database:', error);
  }
}

async function deleteHistoryLevel(){
  const firestore = admin.firestore();
  const collection = firestore.collection('level_history');
  try{
    const currentDate = new Date();
    const tenDaysAgo = new Date(currentDate);
    tenDaysAgo.setDate(currentDate.getDate() - 60);

  const querySnapshot = await collection.where("timeStamp", "<=", tenDaysAgo).get();
  var count = 0;
  for (const doc of querySnapshot.docs) {
    await doc.ref.delete();
    count++;
    console.log("Document successfully deleted!"+count.toString());
  }

  }catch(error){
    console.error('Error deleting history level:', error);
  }
}

const db = admin.firestore();


const generateTimestamps = () => {
  const today = new Date();
  const yesterday = new Date(today);
  yesterday.setDate(today.getDate()-3);

  const timestamps = [];

  for (let i = 0; i < 150; i++) {
    const timestamp = new Date(yesterday);
    timestamp.setMinutes(timestamp.getMinutes() + i * 30);
    timestamps.push(timestamp);
  }

  return timestamps;
};

